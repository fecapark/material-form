{
  "name": "material-todo-list",
  "description": "Material Designed todo list with Typescript, Vite, SCSS, Jest, etc.",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "test": "jest",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview-build": "tsc && vite build --mode preview-build",
    "preview": "yarn preview-build && vite preview --port 3000",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@types/jest": "^27.5.0",
    "gh-pages": "^4.0.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "sass": "^1.51.0",
    "ts-jest": "^28.0.2",
    "typescript": "^4.5.4",
    "vite": "^2.9.9"
  },
  "homepage": "https://fecapark.github.io/material-todo-list/",
  "dependencies": {
    "bezier-easing": "^2.1.0",
    "dotenv": "^16.0.1"
  }
}
